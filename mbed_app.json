{
    "config": {
        "version": "0x00000001",
        "name": "esp8266"
    },
    
    "macros": ["DEBUG_ISR_STACK_USAGE=1", "MBED_HEAP_STATS_ENABLED=1", "MBED_STACK_STATS_ENABLED=1", "MBED_CONF_NSAPI_PRESENT=1"],
    "target_overrides": {
        "*": {
            "mbed-trace.enable": 0,
            "platform.thread-stats-enabled": true,
            "platform.stdio-convert-newlines": "1",
            "platform.stdio-baud-rate": 115200
        }
    }
}
